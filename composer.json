{
  "name": "datenkraft/bb-base-api-php-client",
  "type": "library",
  "description": "API Client that enables you to work with Backbone PHP Client packages",
  "keywords": [
    "apiclient",
    "backbone",
    "php",
    "base"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Datenkraft GmbH"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^6.0 || ^7.3",
    "jane-php/open-api-runtime": "^7.5.0",
    "league/oauth2-client": "^2.6",
    "nyholm/psr7": "^1.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpunit/phpunit": "^9.6",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Datenkraft\\Backbone\\Client\\BaseApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "platform": {
      "php": "8.0"
    }
  }
}
